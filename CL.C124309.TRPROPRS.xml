                    <nciodm:PreferredTerm>Unequivocal</nciodm:PreferredTerm>
                </EnumeratedItem>
                <EnumeratedItem CodedValue="UNEQUIVOCAL PROGRESSION" nciodm:ExtCodeID="C96644">
