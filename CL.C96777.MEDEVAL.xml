                    <nciodm:PreferredTerm>Radiologist 2</nciodm:PreferredTerm>
                </EnumeratedItem>
                <EnumeratedItem CodedValue="RATER" nciodm:ExtCodeID="C132304">
                    <nciodm:CDISCDefinition>An individual qualified to perform an assessment.</nciodm:CDISCDefinition>
                    <nciodm:PreferredTerm>Rater</nciodm:PreferredTerm>
                </EnumeratedItem>
                <EnumeratedItem CodedValue="RATER 1" nciodm:ExtCodeID="C132425">
