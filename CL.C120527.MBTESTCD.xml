                <EnumeratedItem CodedValue="AFB" nciodm:ExtCodeID="C128981">
                    <nciodm:CDISCDefinition>A measurement of any rod-shaped bacteria that resist decolorizing by acid after accepting an aqueous stain solution, due to the mycolic acids in their cell walls.</nciodm:CDISCDefinition>
                    <nciodm:PreferredTerm>Acid-Fast Bacilli Measurement</nciodm:PreferredTerm>
                </EnumeratedItem>
                <EnumeratedItem CodedValue="INFAAG" nciodm:ExtCodeID="C120677">
                    <nciodm:CDISCSynonym>Influenza A Antigen</nciodm:CDISCSynonym>
                    <nciodm:CDISCDefinition>A measurement of the influenza A antigen in a biological specimen.</nciodm:CDISCDefinition>
                    <nciodm:PreferredTerm>Influenza A Antigen Measurement</nciodm:PreferredTerm>
                </EnumeratedItem>
                <EnumeratedItem CodedValue="INFABAG" nciodm:ExtCodeID="C120678">
                    <nciodm:CDISCSynonym>Influenza A/B Antigen</nciodm:CDISCSynonym>
                    <nciodm:CDISCDefinition>A measurement of the influenza A/B antigen in a biological specimen.</nciodm:CDISCDefinition>
                    <nciodm:PreferredTerm>Influenza A/B Antigen Measurement</nciodm:PreferredTerm>
